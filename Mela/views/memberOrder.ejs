    <link rel="stylesheet" href="/stylesheets/MelaStyle.css">
    <link rel="stylesheet" href="/stylesheets/memberindexCH.css">
    <title>會員中心 訂單查詢</title>
</head>
<body>
    <!-- 導覽列 -->
    <%- include('./share/navigation') -%>

    <div class="wrap">
        <div class="container">
            <div class="controltable">
                <img src="/images/member_icon.svg" width="100px">
                <li>
                    <h3>Hello! <%= user %></h3>
                </li>
                <li>
                  
                </li>
                <a href="/users/memberLogout">
                    <button class="btnMela">登出</button>
                </a>
            </div>
            
            <!-- 切換頁籤 -->
            <div class="tablewrap">
                <div class="tabletag">
                    <div class="order"><a href="/users/memberOrder"><h6>訂單查詢</h6></a></div>
                    <div class="event"><a href="/users/memberEvent"><h6>活動報名</h6></a></div>
                    <div class="member"><a href="/users/memberEdit"><h6>會員資料</h6></a></div>
                    <div class="password"><a href="/users/memberPass"><h6>修改密碼</h6></a></div>
                </div>
            <!-- 訂單查詢表格 -->
                <div class="tableinfoorder">
                    <div class="tableinfotag">
                        <h4>訂單查詢</h4>
                    </div>
                    <table class="table  table-hover">
                        <thead>
                          <tr>
                            <th>訂單編號</th>
                            <th>訂購日期</th>
                            <th>付款方式</th>
                            <th>訂購方案</th>
                            <th>訂購金額</th>
                            <th>訂單進度</th>
                          </tr>
                        </thead>
                        <tbody>
                            <% for(let i = 0; i < memberOrderData.length; i++ ) { %>
                          <tr>
                            <td><%= memberOrderData[i].poID %></td>
                            <td><%= memberOrderData[i].poDate %></td>
                            <td><%= memberOrderData[i].poPayMethod %></td>
                            <td><%= memberOrderData[i].pID %></td>
                            <td><%= memberOrderData[i].poPrice %></td>
                            <td><%= memberOrderData[i].poDeliver %></td>
                          </tr>
                          <% } %>                          
                        </tbody>
                      </table>
                </div>
            </div>
        </div>
    </div>
    <!-- 頁尾 -->
    <div class="footer wow fadeInUp">
        <div class="footerimg">
            <img src="/images/footerbackground.png">
            <div class="footerlink">
                <a href=""><h5>購物說明</h5></a>
                <a href=""><h5>|  網站使用條款</h5></a> 
                <a href=""><h5>|  隱私權政策</h5></a>
                <a href=""><h5>|  聯絡我們</h5></a>
            </div>
            <h4>© 2020 Mela 蔬果箱訂閱｜每月小農蔬果新鮮配送到府</h4>
        </div>
    </div>
    <script>
        // 點order頁籤
        $(".order").css("background-color","#8FC31F");
        $(".order a").css("color","white");
        var date = document.getElementById("Date")
        var Str=date.getFullYear();
        cosole.log("OK");
        console.log(Str);
        console.log(date);

        
      
    </script>
</body>
</html>